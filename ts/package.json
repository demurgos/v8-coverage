{
  "name": "@c88/v8-coverage",
  "version": "0.2.0",
  "description": "Helpers for V8 coverage files.",
  "author": "Charles Samborski <demurgos@demurgos.net> (https://demurgos.net)",
  "license": "MIT",
  "keywords": [
    "v8",
    "coverage",
    "c8"
  ],
  "homepage": "https://github.com/demurgos/v8-coverage",
  "repository": {
    "type": "git",
    "url": "git://github.com/demurgos/v8-coverage.git"
  },
  "type": "module",
  "files": [
    "./lib/",
    "./src/lib/"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*"
      ]
    }
  },
  "scripts": {
    "build": "tsc --build src/lib/tsconfig.json",
    "prepack": "yarn run build",
    "test:build": "tsc --build src/test/tsconfig.json",
    "test": "yarn run test:build && mocha \"test/**/*.spec.js\""
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.1",
    "chai": "^4.3.4",
    "mocha": "^9.1.2",
    "typescript": "^4.4.3"
  }
}
